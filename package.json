{
  "name": "hirefrank-www",
  "version": "1.0.0",
  "description": "Frank Harris personal website - Tanstack Start + Cloudflare Workers",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "build:check": "pnpm typecheck && pnpm build",
    "preview": "vite preview",
    "deploy": "pnpm build && wrangler deploy",
    "deploy:check": "pnpm build:check && wrangler deploy",
    "cf-typegen": "wrangler types",
    "postinstall": "pnpm run cf-typegen"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251113.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.139.0",
    "@tanstack/react-start": "^1.139.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.553.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.6.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-plugin-markdown": "^2.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.40.2"
  }
}
