{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "hirefrank-www",
  "compatibility_date": "2025-09-24",
  // Cloudflare account ID
  "account_id": "6034f1c5d23e5503f6573740480cf0d6",
